{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "env": {
        "MONGODB_URL": "@mongodb_url",
        "OPENROUTER_API_KEY": "@openrouter_api_key",
        "OPENROUTER_MODEL": "anthropic/claude-3.5-sonnet"
    },
    "regions": [
        "sin1"
    ]
}